    /* Visit https://aka.ms/tsconfig to read more about this file */
    "module": "nodenext",
    "strict": true,
    "moduleResolution": "nodenext",
    "target": "esnext",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "paths": {
      "~/*": ["./*"],
      "@/shared/*": ["../shared/*"],
    }
